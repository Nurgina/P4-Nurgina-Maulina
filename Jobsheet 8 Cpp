#include<stdio.h>
int main()
{
	printf("Nurgina Maulina\n");
	printf("F1B019110\n");
	printf("Kelompok 19\n\n");
	
	int x[10],i;
	for(i=0; i<=10; i++) {
	
	printf("Masukkan nilai %i:", i+1);
	scanf("%i",&x[i]);
}
   printf("\n");
   for(i=0;i<=10; i++)
   {
   	printf("Nilai ke-% = %i\n", i+1, x[i]);
   }
   printf("\n");
   return 0;
}
